//program to show whether a number is divisible by 5
#include<stdio.h>

int main()
{
    int a;
    printf("enter any number\n");
    scanf("%d",&a);
    
    if(a%5==0)
    {printf("the number is divisible by 5");
    }
    else
    {printf("the number is not divisible by 5");
    }
    
    

    return 0;
}